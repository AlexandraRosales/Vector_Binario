#include <iostream>
using namespace std;
int main (){
int numeros[100], n;

cout<<"VECTOR BIONARIO"<<endl;
cout<<"Digita el numero de elemntos del vector:"<<endl;
cin>>n;


for(int i=0;i<n;i++){
    cout<<"Ingresa los elementos del vector"<<endl;
    cin>>numeros[i];
}
cout<<"Vector resultante:"<<endl;
for(int i=0;i<n;i++){
    if(numeros[i]>=0){
    }   
         else if (numeros[i]<0){
             numeros[i]=0;
         }
    
    cout<<"->"<<numeros[i]<<endl;
 
}
